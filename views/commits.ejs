<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
    </head>

    <body>
        <h1>Commits</h1>
        <table>
            <tr>
                <th>Plugin</th>
                <th>Github Link</th>
            </tr>
            <% if(yml_json.components){ %>
            <% for(let i = 0; i < yml_json.components.length; i++){ %>
            <tr>
                <td><%= yml_json.components[i].name %></td>
                <td><a href="<%= yml_json.components[i].repository.slice(0, -4) + '/commit/' + yml_json.components[i].ref%>">Github</a></td>
            </tr>
            <% } %>
            <% } %>
        </table>
    </body>
</html>